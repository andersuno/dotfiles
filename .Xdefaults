#URxvt*background: rgba:0000/0000/0200/c800
#URxvt*background: [70]#000000
#URxvt.foreground: white
#URxvt.

URxvt*depth: 32
URxvt.scrollBar: false
URxvt.font:         xft:bitstream Vera Sans Mono::bold:size=10:antialias=true
URxvt.boldfont:     xft:bitstream Vera Sans Mono:bold:size=10:antialias=true
#URxvt.font:         xft:inconsolata:pixelsize=13:antialias=true:hinting=true
#URxvt.boldfont:     xft:inconsolata:bold:pixelsize=13:antialias=true:hinting=true
URxvt.letterSpace:  -1
URxvt.lineSpace:    0

! special
*.foreground:   #ababab
*.background:   [95]#000000
*.cursorColor:  #ababab

! black
*.color0:       #000000
*.color8:       #343434

! red
*.color1:       #e92f2f
*.color9:       #e92f2f

! green
*.color2:       #0ed839
*.color10:      #0ed839

! yellow
*.color3:       #dddd13
*.color11:      #dddd13

! blue
*.color4:       #3b48e3
*.color12:      #3b48e3

! magenta
*.color5:       #f996e2
*.color13:      #f996e2

! cyan
*.color6:       #23edda
*.color14:      #23edda

! white
*.color7:       #ababab
*.color15:      #f9f9f9

